<configs>
	
		<append xpath="/blocks">

<block name="lantern2">
		<property name="CreativeMode" value="Player"/>
	<property name="CustomIcon" value="lantern2"/>
	<property name="Shape" value="ModelEntity"/>
	<property name="Model" value="#@modfolder:Resources/lantern2.unity3d?lantern2.prefab"/>
	<property name="Class" value="CarExplodeLoot"/>
	<property name="LootList" value="306"/>
	<property name="DropScale" value="0.5"/>
	<property name="CanPickup" value="true"/>
	<property name="Collide" value="sight,movement,melee,bullet,arrow,rocket"/>
	<property name="Material" value="MFuelBarrelMetal"/>
	<property name="Place" value="TowardsPlacerInverted"/>
	<property name="Path" value="solid"/>
	<property name="IsTerrainDecoration" value="true"/>
	<property name="CanDecorateOnSlopes" value="false"/>
		<property name="EconomicValue" value="50"/>
	<property name="EconomicBundleSize" value="1"/>
	<property name="SellableToTrader" value="true"/>
		<property name="LightValue" value=".35"/>
	<property name="LightSource" value="lightSource"/>
	<property class="Explosion">
		<property name="ParticleIndex" value="10"/>
		<property name="RadiusBlocks" value="2"/>
		<property name="BlockDamage" value="50"/>
<property name="Explosion.BlastPower" value="135"/>
	<property name="Explosion.DamageBonus.stone" value=".1"/>
	<property name="Explosion.DamageBonus.metal" value="0"/>
	<property name="Explosion.DamageBonus.earth" value="0"/>
	<property name="Explosion.DamageBonus.water" value="0"/>
	<property name="Explosion.DamageBonus.wood" value="6"/>
		<property name="RadiusEntities" value="5"/>
		<property name="EntityDamage" value="90"/>
	</property>
	<property name="DescriptionKey" value="cntBarrelGroupDesc"/>
	<drop event="Harvest" name="resourceScrapIron" count="3,6" tag="allToolsHarvest"/>
	<drop event="Destroy" count="0"/>
	<drop event="Fall" name="scrapMetalPile" count="1,5" prob="0.75" stick_chance="1"/>
	<property name="FilterTags" value="fdecor,fother,floot"/>
	</block>
	
