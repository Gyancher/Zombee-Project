	<property name="DropMeshfile" value="#Other/Items?Misc/sack_droppedPrefab.prefab"/>
	<property name="DescriptionKey" value="apparelWhitesongPendantDesc"/>
	<property name="Group" value="Clothing"/>
	<property name="EquipSlot" value="Face"/>
	<property class="UMA">
		<property name="Mesh" value=""/>
		<property name="Overlay0" value=""/>
		<property name="Layer" value="1"/>
		<property name="UISlot" value="Face"/>
		<property name="Mask0" value="Face" param1="base"/>
	</property>
	<effect_group tiered="false">
		<!-- attach prefab -->
		<triggered_effect trigger="onSelfEquipStart" action="AttachPrefabToEntity" prefab="#@modfolder:Resources/MagistersMansion.unity3d?PendantLightPrefab" parent_transform="Hips"/>
		<triggered_effect trigger="onSelfEquipStop" action="RemovePrefabFromEntity" prefab="PendantLightPrefab" parent_transform="Hips"/>
		
		<!-- remove ghost buff -->
		<triggered_effect trigger="onSelfDamagedOther" action="RemoveBuff" buff="buffGhostStatusBuff" target="other"/>
		
		<!-- immunity to ground -->
		<passive_effect name="BuffResistance" operation="base_add" value="2" tags="buffHauntedMansionStart,buffHauntedMansionWarning"/>
		<triggered_effect trigger="onSelfEquipStart" action="RemoveBuff" buff="buffHauntedMansionWarning" target="self"/>
		<triggered_effect trigger="onSelfEquipStart" action="RemoveBuff" buff="buffHauntedMansionStart" target="self"/>
		
		<passive_effect name="ModSlots" operation="base_set" value="0"/>
	</effect_group>
	<effect_group>
			<requirement name="IsNight"/>	
		<passive_effect name="HealthMax" operation="perc_add" value=".20"/>
		<triggered_effect trigger="onSelfDamagedOther" action="AddBuff" buff="buffSpecterAttackBuff" target="other"/>
		<passive_effect name="WalkSpeed" operation="perc_add" value=".15"/>
		<passive_effect name="RunSpeed" operation="perc_add" value=".15"/>
	</effect_group>
