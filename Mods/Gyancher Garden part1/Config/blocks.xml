<configs>
	<append xpath="/blocks">
<block name="Wind Chime">  
    <property name="CreativeMode" value="Player"/>
	<property name="DescriptionKey" value="powerSourceDesc"/>
	<property name="Light" value="1.5"/>
	<property name="CustomIcon" value="chime" />
	<property name="Material" value="Mmetal_thin"/>	
	<property name="Shape" value="ModelEntity"/>
	<property name="Model" value="#@modfolder:Resources/windchime.unity3d?WindChime.prefab"/>
	<property name="ImposterDontBlock" value="false"/>
		<property name="ModelOffset" value="0,1,0" />
	<property name="Collide" value="melee,bullet,arrow,rocket"/>
	<property name="Weight" value="1"/>
	<property name="CanPickup" value="true" param1="Wind Chime"/>
	<property name="EconomicValue" value="40"/> <!-- disassemble value -->
	<property name="FilterTags" value="fdecor,fother,ffurniture"/>										
</block>

