<block name="ButterflyPurple">
	<property name="Material" value="Mmetal_thin"/>
	<property name="StabilitySupport" value="false"/>
		<property name="CustomIcon" value="ButterflyPurple" />
	<property name="Shape" value="ModelEntity"/>
	<property name="Model" value="#@modfolder:Resources/ButterflyPurple.unity3d?ButterflyPurple.prefab"/>
