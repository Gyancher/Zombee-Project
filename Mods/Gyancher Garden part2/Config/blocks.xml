		<block name="Tractor">			
			<property name="DescriptionKey" value="TractorDesc"/>
			<property name="CustomIcon" value="Tractor" />
			<property name="Group" value="Special Items"/>
			<property name="Tags" value="fother"/>
			<property name="Class" value="SecureLoot"/>
			<property name="DisplayType" value="blockHardenedMulti"/>
			<property name="Material" value="Msteel"/>
			<property name="MaxDamage" value="20000"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="Model" value="#@modfolder:Resources/TractorPrefab.unity3d?TractorPrefab"/>
			<property name="MultiBlockDim" value="4,4,4"/>
			<property name="ImposterExchange" value="imposterBlock" param1="18"/>
			<property name="Collide" value="sight,movement,melee,bullet,arrow,rocket"/>
			<property name="IsTerrainDecoration" value="true"/>
			<property name="Place" value="TowardsPlacerInverted"/>
			<property name="LootList" value="421"/>
			<property class="RepairItems"> <property name="resourceForgedIron" value="30"/> </property>
			<drop event="Destroy" count="0"/>
			<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>			
			<property name="SellableToTrader" value="false" />
			<property name="FilterTags" value="floot,ffurniture"/>
		</block>	
		


