		<block name="Morel">
			<property name="CustomIcon" value="Morel" />
			<property name="Material" value="Mmushrooms" />
			<property name="Shape" value="ModelEntity" />
			<property name="Mesh" value="models" />
			<property name="Model" value="#@modfolder:Resources/Morel.unity3d?Morel" /> 
			<!-- <property name="HandleFace" value="Bottom"/> -->
			<property name="ModelOffset" value="0,.5,0" />
			<property name="Collide" value="melee,bullet,arrow,rocket" />
			<property name="CanPickup" value="true" param1="Morel" />
			<property name="IsTerrainDecoration" value="true" />
			<property name="HarvestOverdamage" value="false" />
			<drop event="Destroy" name="Morel" count="1" />
		</block>
		
	
<block name="scarecrow">
	<property name="Material" value="Mconcrete" />
