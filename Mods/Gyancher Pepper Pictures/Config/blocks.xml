<Config>
	
<append xpath="/blocks">
	
<block name="Picture Pepper 1">
	<property name="CustomIcon" value="Pepper1"/>
	<property name="CreativeMode" value="Player"/>
	<property name="Material" value="Mglass"/>
	<property name="StabilitySupport" value="false"/>
	<property name="Shape" value="ModelEntity"/>
	<property name="Model" value="#@modfolder:Resources/Pepper1.unity3d?Pepper1"/>
	<property name="Place" value="TowardsPlacerInverted"/>
	<property name="IsDecoration" value="true"/>
	<property name="ImposterDontBlock" value="true"/>
	<drop event="Destroy" name="resourceBrokenGlass" count="5,10"/>
	<drop event="Destroy" name="resourceWood" count="5,10"/>
	<drop event="Destroy" name="resourceNail" count="1,2"/>
	<property name="FilterTags" value="fdecor,fother"/>
	<property name="SortOrder1" value="7000"/>
	<property name="SortOrder2" value="0002"/>
	<property name="DescriptionKey" value="hiddenSafePaintingGroupDesc"/>
</block>


